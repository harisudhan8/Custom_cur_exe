{
    "manifest_version": 3,
    "name": "Custom Cursor",
    "version": "1.0",
    "description": "Upload and apply a custom cursor.",
    "permissions": ["storage", "scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Custom Cursor"
    }
  }
  